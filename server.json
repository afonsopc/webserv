{
	"servers": [
		{
			"port": 8080,
			"host": "0.0.0.0",
			"routes": [
				{
					"path": "/omelhorsite/*",
					"redirect": "https://omelhorsite.pt",
					"methods": [
						"GET"
					]
				},
				{
					"path": "/pagman",
					"index": [
						"index.html", "index.htm"
					],
					"directory": "www",
					"methods": [
						"GET"
					]
				},
				{
					"path": "/cgi/*",
					"directory": "www/cgi",
					"cgi": true,
					"methods": ["GET", "POST"]
				},
				{
					"path": "/*",
					"directory": "www",
					"directory_listing": true,
					"methods": [
						"GET",
						"POST"
					]
				}
			],
			"error_pages": [
				{
					"code": 404,
					"page": "/404.html"
				}
			]
		},
		{
			"port": 1143,
			"host": "0.0.0.0",
			"routes": [
				{
					"path": "/*",
					"directory": "www/cgi",
					"index": ["proxy"],
					"cgi": true,
					"methods": ["GET", "POST", "DELETE", "PUT", "PATCH", "OPTIONS"]
				}
			]
		}
	]
}
