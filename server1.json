{
  "servers": [
    {
      "port": 8080,
      "host": "localhost",
      "root": "www",
      "index": ["index.html"],
      "locations": [
        {
          "path": "/",
          "try_files": "$uri $uri/ =404"
        }
      ],
      "error_pages": [
        {
          "code": 404,
          "page": "/404.html"
        }
      ]
    }
  ]
}